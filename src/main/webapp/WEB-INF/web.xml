<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>editProfileGrades</servlet-name>
        <servlet-class>acad.db.courserank.editProfileGrades</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>authenticate</servlet-name>
        <servlet-class>acad.db.courserank.dbis.Authenticate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>search</servlet-name>
        <servlet-class>acad.db.courserank.dbis.Search</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loadImage</servlet-name>
        <servlet-class>acad.db.courserank.dbis.loadImage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>submitFeedback</servlet-name>
        <servlet-class>acad.db.courserank.submitFeedback</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registerOffering</servlet-name>
        <servlet-class>acad.db.courserank.registerOffering</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editProfileProjects</servlet-name>
        <servlet-class>acad.db.courserank.editProfileProjects</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editProfilePassword</servlet-name>
        <servlet-class>acad.db.courserank.editProfilePassword</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>editProfileGrades</servlet-name>
        <url-pattern>/editProfileGrades</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>authenticate</servlet-name>
        <url-pattern>/authenticate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>search</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loadImage</servlet-name>
        <url-pattern>/loadImage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>submitFeedback</servlet-name>
        <url-pattern>/submitFeedback</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registerOffering</servlet-name>
        <url-pattern>/registerOffering</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editProfileProjects</servlet-name>
        <url-pattern>/editProfileProjects</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editProfilePassword</servlet-name>
        <url-pattern>/editProfilePassword</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
